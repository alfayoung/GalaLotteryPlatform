# Backend Environment Variables
# Copy this file to .env and fill in your actual values

# Server Configuration
SERVER_PORT=7500

# SSL Configuration (Optional)
SSL_ENABLED=false
SSL_KEY_STORE=classpath:your-domain.jks
SSL_KEY_PASSWORD=your-keystore-password

# SJTU JAccount OAuth2
OAUTH_CLIENT_ID=your-client-id
OAUTH_CLIENT_SECRET=your-client-secret

# Admin Authentication
ADMIN_PASSWORD=your-admin-password

# MySQL Database
DB_URL=jdbc:mysql://localhost:3306/lottery?serverTimezone=UTC&useUnicode=true&characterEncoding=utf-8&useSSL=true
DB_USERNAME=root
DB_PASSWORD=your-db-password
